<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yunji.titanrtx.manager.dao.mapper.report.BeaconCollectorMapper">


    <select id="findAlertsBetweenTime" resultType="com.yunji.titanrtx.manager.dao.entity.data.BeaconAlertEntity">
        SELECT alert_id, alert_time,msg FROM t_notice_history
        WHERE
            alert_time BETWEEN  #{startTime} AND  #{endTime}
        AND
            alert_id LIKE #{alertId}
    </select>

</mapper>

